{
  "name": "codecept_reference",
  "version": "1.0.0",
  "description": "A codecept.js reference project",
  "main": "",
  "scripts": {
    "publisher3_prod": "rimraf allure-results && npx codecept_tools run --config ./tests/publisher3/config_prod.json --async",
    "publisher3_demo": "rimraf allure-results && npx codecept_tools run --config ./tests/publisher3/config_demo.json --async",
    "prod_verbose": "rimraf allure-results && npx codecept_tools run --params '--debug,--steps' --config ./tests/publisher3/config_prod.json --async --verbose",
    "demo_verbose": "rimraf allure-results && npx codecept_tools run --params '--debug,--steps' --config ./tests/publisher3/config_demo.json --async --verbose",
    "report": "rimraf allure-report && allure generate allure-results && allure open",
    "selenium_start": "selenium-standalone start",
    "deploy": "npm install &&  selenium-standalone install"
  },
  "author": "Mustaev Denis <d.mustaev@xsolla.com>",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.5.0",
    "codecept_tools": "git+https://github.com/nsected/codecept_tools.git",
    "codeceptjs": "^1.1.7",
    "glob": "^7.1.2",
    "mocha-allure-reporter": "^1.3.2",
    "mocha-multi": "^1.0.1",
    "rimraf": "^2.6.2",
    "selenium-standalone": "^6.14.0",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "0.0.10",
    "webdriverio": "^4.12.0",
    "xsolla-community-helpers": "git+https://github.com/nsected/xsolla-community-helpers.git"
  }
}
